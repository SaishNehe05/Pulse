{
  "expo": {
    "name": "Pulse",
    "slug": "Pulse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pulse",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saish.pulse",
      "googleServiceInfoPlist": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.saish.pulse",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#0E0D22",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F4F5F7",
          "dark": {
            "backgroundColor": "#11131A"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Pulse to access your photos to share content.",
          "cameraPermission": "Allow Pulse to access your camera to take photos for posts."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/android-icon-monochrome.png",
          "color": "#5E9BFF",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "#{unread_notifications} new notifications",
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Pulse to access your photos.",
          "savePhotosPermission": "Allow Pulse to save photos to your library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8271372d-7662-4edb-9e80-021e33765c4c"
      }
    }
  }
}